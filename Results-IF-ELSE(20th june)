print("Enter marks");

s1=int(input("S1: "));

s2=int(input("S2: "));

s3=int(input("S3: "));

s4=int(input("S4: "));

s5=int(input("S5: "));

perc=(s1+s2+s3+s4+s5)/5;

print("Percentage:",perc)

if(0>=perc>35):

print("Fail");

elif(35>=perc<40):

print("Pass class");

elif(40<=perc<60):

print("2nd class");

elif(60<=perc<75):

print("1st class");

elif(75<=perc<100):

print("distinction")
